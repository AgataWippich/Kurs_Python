lista = [1, 2, 3, 4, 5, 7, 8, 9, 11, 13, 21, 27, 28, 31]
liczby_pierwsze = []

for el in lista:
    if el == 1: continue
    for x in range(2, el):
        if el % x == 0:
            print(f"Znalaz≈Çem dzielnik {x} dla {el}")
            break
    else:
        liczby_pierwsze.append(el)


print(liczby_pierwsze)
